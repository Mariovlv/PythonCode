# En lo siguiente:
# a, b son los puntos del intervalo
# n es el n√∫mero de particiones

import math

def sumRiemann(a, b, n):
    sumat = 0
    for i in range(n):
        particion = (b-a)/n
        x = a + (i*particion)
        funcion = 1/x
        suma = funcion * particion
        sumat += suma

    return sumat

print("El resultado es:", sumRiemann(1, 2, 1000))
